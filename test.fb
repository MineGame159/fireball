struct Vec2 {
    x i32
    y i32
}

func main() i32 {
    var point = getPoint();
    modifyPoint(&point);

    printf("X: %d, Y: %d\n", point.x, point.y);

    return 0;
}

func getPoint() Vec2 {
    var point Vec2;

    point.x = 1;
    point.y = 3;

    return point;
}

func modifyPoint(point *Vec2) {
    point.y *= 2;
}

extern func printf(format *u8, ...) void
