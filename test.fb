struct Vec2 {
    x i32
    y i32
}

func main() i32 {
    var point = newPoint(2, 6);

    printf("X: %d, Y: %d\n", point.x, point.y);

    free(point);
    return 0;
}

func newPoint(x i32, y i32) *Vec2 {
    var point = malloc(8 as u64) as *Vec2;

    point.x = x;
    point.y = y;

    return point;
}

extern func printf(format *u8, ...) void

extern func malloc(size u64) *void
extern func free(ptr *void) void
