struct Point {
    i32 x;
    i32 y;

    Point(i32 x, i32 y) {
        this.x = x;
        this.y = y;
    }

    void add(i32 x, i32 y) {
        this.x = this.x + x;
        this.y = this.y + y;
    }
}

i32 main() {
    // Basic
    var b = getNumber();
    b = 6 / 2;

    if (true) {}
    else {}

    print(b);

    // Structs
    var p = Point(0, 10);
    p.add(1, 0);
    print2(p.x, p.y);

    return 0;
}

i32 getNumber() {
    return 8;
}

void modify(Point* p) {
    p.x = 6;
    p.y = 9;
}

/*
 * C functions
 */

void print2(i32 x, i32 y) {
    c{ printf("[%d, %d]\n", x, y); }
}

void print(i32 number) {
    c{ printf("%d\n", number); }
}