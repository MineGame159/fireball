struct Vec2 {
    x i32,
    y i32,
}

enum Animal {
    Dog = 9,
    Cat,
}

func main() i32 {
    printVec(Vec2 { x: 2, y: 5 });
    printVec(Vec2 { x: 2 });
    printVec(Vec2 { y: 5 });
    printVec(Vec2 {});

    return 0;
}

func printVec(vec Vec2) {
    printf("X: %d, Y: %d\n", vec.x, vec.y);
}
