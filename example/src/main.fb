struct Vec2 {
    x i32,
    y i32,
}

impl Vec2 {
    func add(v Vec2) Vec2 {
        return Vec2 { x: this.x + v.x, y: this.y + v.y };
    }

    func print() {
        printf("X: %d, Y: %d\n", this.x, this.y);
    }
}

func main() {
    var a = Vec2 { x: 2, y: 9 };
    var b = Vec2 { x: 6, y: 2 };

    a.add(b).print();
}
